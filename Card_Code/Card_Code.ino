// Utilities
///
/// @brief Wait with countdown
/// @param second duration, s
///
void wait(uint8_t second)
{
    for (uint8_t i = second; i > 0; i--)
    {
        Serial.print(formatString(" > %i  \r", i));
        delay(1000);
    }
    Serial.print("         \r");
}


// Functions
///
/// @brief Basic forms test screen
/// @param flag true = default = perform flush, otherwise no
///
/// @image html T2_FORMS.jpg
/// @image latex T2_FORMS.PDF width=10cm
///

void displayForms(bool flag = true)
{
    myScreen.setOrientation(0);
    char c;
    int row = 0;
    int col = 0;
    for(int i = 0;i < 5808; i++){
      if(i%22  == 0){ //when 16 bytes are read, a full line will have been displayed
        row++;
        col = 0; //reset the column to zero
      }
      for(int j = 0; j < 8; j++){
         c = epd_bitmap_arduino[i]; //get character
         
          if(bitRead(c,7-j) == 1){
            myScreen.point(col*8+j,row ,myColours.black);
          }else{
            myScreen.point(col*8+j,row,myColours.white);
          }
      }
      col++;
    }

    myScreen.flush();
}
    




